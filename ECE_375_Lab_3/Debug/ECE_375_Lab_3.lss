000107 951a                                 dec     r17
000108 f7e9                                 brne    CLRloop2
                                 
000109 911f                      	   pop	   r17
00010a 910f                      	   pop	   r16
00010b 91af                      	   pop	   XL
00010c 91bf                      	   pop	   XH
                                 
00010d 9508                                 ret
