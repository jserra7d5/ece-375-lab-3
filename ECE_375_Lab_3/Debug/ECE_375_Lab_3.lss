000079 d07b                      		rcall LCDClr
00007a e5e4                      		ldi ZL, low(STRING_ONE * 2)
00007b e0f1                      		ldi ZH, high(STRING_ONE * 2)
                                 
00007c e0c0                      		ldi YL, low(0x0100)
00007d e0d1                      		ldi YH, high(0x0100)
00007e d007                      		rcall COPY_LOOP
                                 
00007f e6e0                      		ldi ZL, low(STRING_TWO * 2)
000080 e0f1                      		ldi ZH, high(STRING_TWO * 2)
                                 
000081 e1c0                      		ldi YL, low(0x0110)
000082 e0d1                      		ldi YH, high(0x0110)
000083 d002                      		rcall COPY_LOOP
000084 d057                      		rcall LCDWrite
000085 9508                      		ret
                                 
                                 
