                                 ;***********************************************************
                                 ;*	Internal Register Definitions and Constants
                                 ;***********************************************************
                                 .def	mpr = r16				; Multipurpose register is required for LCD Driver
                                 
                                 .def	waitcnt = r17				; Wait Loop Counter
                                 .def	ilcnt = r18				; Inner Loop Counter
                                 .def	olcnt = r19				; Outer Loop Counter
                                 
                                 .equ	WTime = 25				; Time to wait in wait loop
                                 .equ clear_button = 4 ; clear screen button input bit
                                 .equ display_button = 5 ; display button input bit
                                 .equ scroll_button = 7 ; scroll button input bit
                                 
                                 ;***********************************************************
                                 ;*	Start of Code Segment
                                 ;***********************************************************
                                 .cseg							; Beginning of code segment
                                 
                                 ;***********************************************************
                                 ;*	Interrupt Vectors
                                 ;***********************************************************
                                 .org	$0000					; Beginning of IVs
000000 c055                      		rjmp INIT				; Reset interrupt
                                 
                                 .org	$0056					; End of Interrupt Vectors
                                 
                                 ;***********************************************************
                                 ;*	Program Initialization
                                 ;***********************************************************
                                 INIT:							; The initialization routine
                                 		; initialize our stack pointer
000056 ef0f                      		ldi mpr, low(RAMEND)
000057 bf0d                      		out SPL, mpr
000058 e00a                      		ldi mpr, high(RAMEND)
000059 bf0e                      		out SPH, mpr
                                 
                                 		; initialize Port D for input
00005a e000                      		ldi mpr, $00
00005b b90a                      		out DDRD, mpr
00005c ef0f                      		ldi mpr, $FF
00005d b90b                      		out PORTD, mpr
                                 
                                 		; Initialize LCD Display
00005e d051                      		rcall LCDInit
00005f d022                      		rcall CLEAR ; clears garbage values
                                 
                                 		; NOTE that there is no RET or RJMP from INIT,
                                 		; this is because the next instruction executed is the
                                 		; first instruction of the main program
                                 
                                 ;***********************************************************
                                 ;*	Main Program
                                 ;***********************************************************
                                 MAIN:							; The Main program
                                 		; first lets grab our inputs
000060 b109                      		in mpr, PIND
000061 7b00                      		andi mpr, (1<<clear_button | 1<<display_button | 1<<scroll_button)
                                 
                                 		; check if our clear button is pressed, if so, clear the screen
000062 ff04                      		sbrs mpr, clear_button
000063 d01e                      		rcall CLEAR
                                 
                                 		; check if our display button is pressed, if so, display the lines
000064 ff05                      		sbrs mpr, display_button
000065 d012                      		rcall DISPLAY
                                 
                                 		; check if our scroll button is pressed, if so, call the scroll subroutine
000066 ff07                      		sbrs mpr, scroll_button
000067 d026                      		rcall SCROLL
000068 cff7                      		rjmp MAIN			; jump back to main and create an infinite
                                 								; while loop.  Generally, every main program is an
                                 								; infinite while loop, never let the main program
                                 								; just run off
                                 
                                 ;----------------------------------------------------------------
                                 ; Sub:	Wait
                                 ; Desc:	A wait loop that is 16 + 159975*waitcnt cycles or roughly
                                 ;		waitcnt*10ms.  Just initialize wait for the specific amount
                                 ;		of time in 10ms intervals. Here is the general eqaution
                                 ;		for the number of clock cycles in the wait loop:
                                 ;			(((((3*ilcnt)-1+4)*olcnt)-1+4)*waitcnt)-1+16
                                 ;----------------------------------------------------------------
                                 Wait:
000069 931f                      		push	waitcnt			; Save wait register
00006a 932f                      		push	ilcnt			; Save ilcnt register
00006b 933f                      		push	olcnt			; Save olcnt register
                                 
00006c ee30                      Loop:	ldi		olcnt, 224		; load olcnt register
00006d ee2d                      OLoop:	ldi		ilcnt, 237		; load ilcnt register
00006e 952a                      ILoop:	dec		ilcnt			; decrement ilcnt
00006f f7f1                      		brne	ILoop			; Continue Inner Loop
000070 953a                      		dec		olcnt		; decrement olcnt
000071 f7d9                      		brne	OLoop			; Continue Outer Loop
000072 951a                      		dec		waitcnt		; Decrement wait
000073 f7c1                      		brne	Loop			; Continue Wait loop
                                 
000074 913f                      		pop		olcnt		; Restore olcnt register
000075 912f                      		pop		ilcnt		; Restore ilcnt register
000076 911f                      		pop		waitcnt		; Restore wait register
000077 9508                      		ret				; Return from subroutine
                                 
                                 ;-----------------------------------------------------------
                                 ; Func: DISPLAY
                                 ; Desc: Macro to call custom CLEAR subroutine as well as LCDWrite
                                 ;-----------------------------------------------------------
                                 DISPLAY:
000078 d009                      		rcall CLEAR
000079 d05c                      		rcall LCDWrite
00007a 9508                      		ret
                                 
                                 
                                 ;-----------------------------------------------------------
                                 ; Func: COPY_LOOP
                                 ; Desc: Copies from program memory to the memory pointed to by Y.
                                 ;-----------------------------------------------------------
                                 COPY_LOOP:
00007b 95c8                      		lpm ; loads the byte pointed to in Z register into r0
00007c 2000                      		tst r0 ; sets the flag if the byte in r0 is equal to 0 or negative
00007d f019                      		breq END_COPY ; checks the flag, if set, then end copy
                                 
00007e 9209                      		st Y+, r0 ; stores r0 in y, then increment y
00007f 9631                      		adiw ZL, 1 ; increments ZL pointer
000080 cffa                      		RJMP COPY_LOOP ; repeat until 0
                                 
                                 ;-----------------------------------------------------------
                                 ; Func: END_COPY
                                 ; Desc: COPY_LOOP helper function
                                 ;-----------------------------------------------------------
                                 END_COPY:							; Begin a function with a label
000081 9508                      		ret						; End a function with RET
                                 
                                 ;-----------------------------------------------------------
                                 ; Func: CLEAR
                                 ; Desc: Clears the screen, then copies back over our strings
                                 ;-----------------------------------------------------------
                                 CLEAR:
000082 d06c                      		rcall LCDClr ; clear our LCD to get rid of any stray characters
000083 e4e6                      		ldi ZL, low(STRING_ONE * 2) ; load the first 8 bits of our string_one ptr into ZL (r30)
000084 e0f1                      		ldi ZH, high(STRING_ONE * 2) ; load the last 8 bits of our string_one ptr into ZH (r31)
                                 
000085 e0c0                      		ldi YL, low(0x0100) ; load the first 8 bits of our SRAM upper 16 character ptr into YL (r28)
000086 e0d1                      		ldi YH, high(0x0100) ; load the last 8 bits of our SRAM upper 16 character ptr into YL (r29)
000087 dff3                      		rcall COPY_LOOP ; call copy subroutine
                                 
000088 e5e4                      		ldi ZL, low(STRING_TWO * 2) ; load the first 8 bits of our string_two ptr into ZL (r30)
000089 e0f1                      		ldi ZH, high(STRING_TWO * 2) ; load the last 8 bits of our string_two ptr into ZH (r31)
                                 
00008a e1c0                      		ldi YL, low(0x0110) ; load the first 8 bits of our SRAM lower 16 character ptr into YL (r28)
00008b e0d1                      		ldi YH, high(0x0110) ; load the last 8 bits of our SRAM lower 16 character ptr into YL (r29)
00008c dfee                      		rcall COPY_LOOP ; call copy subroutine
00008d 9508                      		ret
                                 
                                 
                                 ;-----------------------------------------------------------
                                 ; Func: SCROLL and SHIFT_LOOP
                                 ; Desc: a function to shift the string on the LCD
                                 ;-----------------------------------------------------------
                                 SCROLL:                          
                                     ; step 1: store last character of line 2 (0x011F)
00008e e1ef                          ldi ZL, low(0x011F)           
00008f e0f1                          ldi ZH, high(0x011F)
000090 8100                          ld mpr, Z                   ; store last character in mpr BEFORE shifting
000091 930f                      	push mpr					; saving our last character value in the stack
                                 
                                     ; step 2: initialize pointers for shifting
000092 e2c0                          ldi YL, low(0x0120)           ; start ONE PAST last char (Y)
000093 e0d1                          ldi YH, high(0x0120)
                                 
                                     ; step 3: shift all characters right by 1 (going backwards)
000094 e200                      	ldi mpr, 32
                                 	SHIFT_LOOP:
000095 930f                      		push mpr					; save our counter value
000096 9102                      		ld mpr, -Z                   ; pre-decrement Z, then load
000097 930a                      		st -Y, mpr					; shifting our value
000098 910f                      		pop mpr                   ; pre-decrement Y, then store
000099 950a                      		dec mpr                      ; decrement counter
00009a f7d1                      		brne SHIFT_LOOP               ; repeat until all 32 characters are shifted
                                 
                                 		; step 4: restore the saved last character to the first position (0x0100)
00009b e0c0                      		ldi YL, low(0x0100)            ; set Y pointer to first char
00009c e0d1                      		ldi YH, high(0x0100)
00009d 910f                      		pop mpr							; grab our last character value
00009e 8308                      		st Y, mpr                      ; store saved last character
                                 
                                 		; step 5: wait to create scrolling effect
00009f d036                      		rcall LCDWrite
0000a0 e119                      		ldi waitcnt, WTime
0000a1 dfc7                      		rcall wait
0000a2 9508                      		ret
                                 
                                 
                                 
                                 
                                 
