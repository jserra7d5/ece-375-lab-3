000086 95c8                      		lpm
000087 2000                      		tst r0
000088 f019                      		breq END_COPY
                                 
000089 9209                      		st Y+, r0
00008a 9631                      		adiw ZL, 1
00008b cffa                      		RJMP COPY_LOOP
                                 
