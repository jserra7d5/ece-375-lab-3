                                 ;*****************************************************************
                                 ;* Func:	Bin2ASCII
                                 ;* Desc:	Converts a binary number into an ASCII 
                                 ;*			text string equivalent. 
                                 ;*			- The binary number needs to be in the mpr
                                 ;*			- The Start Address of where the text will
                                 ;*			 	be placed needs to be in the X Register
                                 ;*			- The count of the characters created are 
                                 ;*				added to the count register
                                 ;*****************************************************************
                                 Bin2ASCII:
000124 930f                      	push	mpr			; save mpr
000125 936f                      	push	r			; save r
000126 935f                      	push	q			; save q
000127 93bf                      	push	XH			; save X-pointer
000128 93af                      	push	XL			;
                                 
                                 	; Determine the range of mpr
000129 3604                      	cpi	mpr, 100		; is mpr >= 100
00012a f018                      	brlo	B2A_1			; goto next check
00012b e043                      	ldi	count, 3		; Three chars are written
00012c 9613                      	adiw	XL, 3			; Increment X 3 address spaces
00012d c007                      	rjmp	B2A_3			; Continue with program
                                 B2A_1:
00012e 300a                      	cpi	mpr, 10		; is mpr >= 10
00012f f018                      	brlo	B2A_2			; Continue with program
000130 e042                      	ldi	count, 2		; Two chars are written
000131 9612                      	adiw	XL, 2			; Increment X 2 address spaces
000132 c002                      	rjmp	B2A_3			; Continue with program
                                 B2A_2:
000133 9611                      	adiw	XL, 1			; Increment X 1 address space
000134 e041                      	ldi	count, 1		; One char is written
                                 
                                 B2A_3:
                                 	; Do-While statement that converts Binary to ASCII
000135 d076                      	rcall	div10			; Call the div10 function
000136 e300                      	ldi	mpr, '0'		; Set the base ASCII integer value
000137 0f06                      	add	mpr, r		; Create the ASCII integer value
000138 930e                      	st	-X, mpr		; Load ASCII value to memory
000139 2f05                      	mov	mpr, q		; Set mpr to quotient value
00013a 3000                      	cpi	mpr, 0		; does mpr == 0
00013b f7c9                      	brne	B2A_3			; do while (mpr != 0)
                                 
00013c 91af                      	pop	XL			; restore X-pointer
00013d 91bf                      	pop	XH			;
00013e 915f                      	pop	q			; restore q
00013f 916f                      	pop	r			; restore r
000140 910f                      	pop	mpr			; restore mpr
000141 9508                      	ret				; return from function
                                 
