000103 93af                      	   PUSH	   XL
000104 930f                                 push    r16
000105 931f                                 push    r17
                                 
000106 e0b1                                 ldi     XH, HIGH(lcd_buffer_addr+16)
000107 e1a0                                 ldi     XL, LOW(lcd_buffer_addr+16)
000108 e110                                 ldi     r17, 16
000109 e200                                 ldi     r16, ' '
                                    CLRloop2:
00010a 930d                                 st      X+, r16         ; load data to DM
00010b 951a                                 dec     r17
00010c f7e9                                 brne    CLRloop2
                                 
00010d 911f                      	   pop	   r17
00010e 910f                      	   pop	   r16
00010f 91af                      	   pop	   XL
000110 91bf                      	   pop	   XH
                                 
000111 9508                                 ret
