                                 ;----------------------------------------------------------------
                                 ; Sub:	Wait
                                 ; Desc:	A wait loop that is 16 + 159975*waitcnt cycles or roughly
                                 ;		waitcnt*10ms.  Just initialize wait for the specific amount
                                 ;		of time in 10ms intervals. Here is the general eqaution
                                 ;		for the number of clock cycles in the wait loop:
                                 ;			(((((3*ilcnt)-1+4)*olcnt)-1+4)*waitcnt)-1+16
                                 ;----------------------------------------------------------------
                                 Wait:
000069 931f                      		push	waitcnt			; Save wait register
00006a 932f                      		push	ilcnt			; Save ilcnt register
00006b 933f                      		push	olcnt			; Save olcnt register
                                 
00006c ee30                      Loop:	ldi		olcnt, 224		; load olcnt register
00006d ee2d                      OLoop:	ldi		ilcnt, 237		; load ilcnt register
00006e 952a                      ILoop:	dec		ilcnt			; decrement ilcnt
00006f f7f1                      		brne	ILoop			; Continue Inner Loop
000070 953a                      		dec		olcnt		; decrement olcnt
000071 f7d9                      		brne	OLoop			; Continue Outer Loop
000072 951a                      		dec		waitcnt		; Decrement wait
000073 f7c1                      		brne	Loop			; Continue Wait loop
                                 
000074 913f                      		pop		olcnt		; Restore olcnt register
000075 912f                      		pop		ilcnt		; Restore ilcnt register
000076 911f                      		pop		waitcnt		; Restore wait register
000077 9508                      		ret				; Return from subroutine
                                 
                                 ;-----------------------------------------------------------
                                 ; Func: DISPLAY
                                 ; Desc: Cut and paste this and fill in the info at the
                                 ;		beginning of your functions
                                 ;-----------------------------------------------------------
                                 DISPLAY:
000078 d009                      		rcall CLEAR
000079 d058                      		rcall LCDWrite
00007a 9508                      		ret
                                 
                                 
                                 ;-----------------------------------------------------------
                                 ; Func: COPY_LOOP
                                 ; Desc: Cut and paste this and fill in the info at the
                                 ;		beginning of your functions
                                 ;-----------------------------------------------------------
                                 COPY_LOOP:
00007b 95c8                      		lpm ; loads the byte pointed to in Z register into r0
00007c 2000                      		tst r0 ; sets the flag if the byte in r0 is equal to 0 or negative
00007d f019                      		breq END_COPY ; checks the flag, if set, then end copy
                                 
00007e 9209                      		st Y+, r0 ; stores r0 in y, then increment y
00007f 9631                      		adiw ZL, 1 ; increments ZL pointer
000080 cffa                      		RJMP COPY_LOOP ; repeat until 0
                                 
                                 ;-----------------------------------------------------------
                                 ; Func: END_COPY
                                 ; Desc: return function
                                 ;-----------------------------------------------------------
                                 END_COPY:							; Begin a function with a label
000081 9508                      		ret						; End a function with RET
                                 
                                 ;-----------------------------------------------------------
                                 ; Func: CLEAR
                                 ; Desc: Cut and paste this and fill in the info at the
                                 ;		beginning of your functions
                                 ;-----------------------------------------------------------
                                 CLEAR:
000082 d068                      		rcall LCDClr ; clear our LCD to get rid of any stray characters
000083 e3ee                      		ldi ZL, low(STRING_ONE * 2) ; load the first 8 bits of our string_one ptr into ZL (r30)
000084 e0f1                      		ldi ZH, high(STRING_ONE * 2) ; load the last 8 bits of our string_one ptr into ZH (r31)
                                 
000085 e0c0                      		ldi YL, low(0x0100) ; load the first 8 bits of our SRAM upper 16 character ptr into YL (r28)
000086 e0d1                      		ldi YH, high(0x0100) ; load the last 8 bits of our SRAM upper 16 character ptr into YL (r29)
000087 dff3                      		rcall COPY_LOOP ; call copy subroutine
                                 
000088 e4ec                      		ldi ZL, low(STRING_TWO * 2) ; load the first 8 bits of our string_two ptr into ZL (r30)
000089 e0f1                      		ldi ZH, high(STRING_TWO * 2) ; load the last 8 bits of our string_two ptr into ZH (r31)
                                 
00008a e1c0                      		ldi YL, low(0x0110) ; load the first 8 bits of our SRAM lower 16 character ptr into YL (r28)
00008b e0d1                      		ldi YH, high(0x0110) ; load the last 8 bits of our SRAM lower 16 character ptr into YL (r29)
00008c dfee                      		rcall COPY_LOOP ; call copy subroutine
00008d 9508                      		ret
                                 
                                 
                                 ;-----------------------------------------------------------
                                 ; Func: SCROLL and SHIFT_LOOP
                                 ; Desc: a function to imi
                                 ;-----------------------------------------------------------
                                 SCROLL:                          ; begin function with a label
                                     ; step 1: store last character of line 2 (0x011F)
00008e e1ef                          ldi ZL, low(0x011F)           
00008f e0f1                          ldi ZH, high(0x011F)
000090 8100                          ld mpr, Z                    ; store last character in mpr BEFORE shifting
                                 
                                     ; step 2: initialize pointers for shifting
000091 e2c0                          ldi YL, low(0x0120)           ; start ONE PAST last char (Y)
000092 e0d1                          ldi YH, high(0x0120)
                                 
                                     ; step 3: shift all characters right by 1 (going backwards)
000093 e280                          ldi mpr2, 32                  ; loop counter (32 shifts)
                                 
                                 	SHIFT_LOOP:
000094 9182                      		ld mpr2, -Z                   ; pre-decrement Z, then load
000095 938a                      		st -Y, mpr2                   ; pre-decrement Y, then store
000096 958a                      		dec mpr2                      ; decrement counter
000097 f7e1                      		brne SHIFT_LOOP               ; repeat until all 32 characters are shifted
                                 
                                 		; step 4: restore the saved last character to the first position (0x0100)
000098 e0c0                      		ldi YL, low(0x0100)            ; set Y pointer to first char
000099 e0d1                      		ldi YH, high(0x0100)
00009a 8308                      		st Y, mpr                      ; store saved last character
                                 
                                 		; step 5: wait to create scrolling effect
00009b d036                      		rcall LCDWrite
00009c e119                      		ldi waitcnt, WTime
00009d dfcb                      		rcall wait
00009e 9508                      		ret
                                 
                                 
                                 
                                 
                                 
