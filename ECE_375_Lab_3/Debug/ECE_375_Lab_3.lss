00008d d067                      		rcall LCDClr
00008e e5e4                      		ldi ZL, low(STRING_ONE * 2)
00008f e0f1                      		ldi ZH, high(STRING_ONE * 2)
                                 
000090 e0c0                      		ldi YL, low(0x0100)
000091 e0d1                      		ldi YH, high(0x0100)
000092 dff3                      		rcall COPY_LOOP
                                 
000093 e6e0                      		ldi ZL, low(STRING_TWO * 2)
000094 e0f1                      		ldi ZH, high(STRING_TWO * 2)
                                 
000095 e1c0                      		ldi YL, low(0x0110)
000096 e0d1                      		ldi YH, high(0x0110)
000097 dfee                      		rcall COPY_LOOP
000098 9508                      		ret
                                 
                                 
                                 SCROLL:                          ; begin function with a label
                                     ; step 1: store last character of line 2 (0x011F)
000099 e1ef                          ldi ZL, low(0x011F)           
00009a e0f1                          ldi ZH, high(0x011F)
00009b 8100                          ld mpr, Z                    ; store last character in mpr BEFORE shifting
                                 
                                     ; step 2: initialize pointers for shifting
00009c e2c0                          ldi YL, low(0x0120)           ; start ONE PAST last char (Y)
00009d e0d1                          ldi YH, high(0x0120)
                                 
                                     ; step 3: shift all characters right by 1 (going backwards)
00009e e280                          ldi mpr2, 32                  ; loop counter (32 shifts)
                                 
                                 SHIFT_LOOP:
00009f 9182                          ld mpr2, -Z                   ; pre-decrement Z, then load
0000a0 938a                          st -Y, mpr2                   ; pre-decrement Y, then store
0000a1 958a                          dec mpr2                      ; decrement counter
0000a2 f7e1                          brne SHIFT_LOOP               ; repeat until all 32 characters are shifted
                                 
                                     ; step 4: restore the saved last character to the first position (0x0100)
0000a3 e0c0                          ldi YL, low(0x0100)            ; set Y pointer to first char
0000a4 e0d1                          ldi YH, high(0x0100)
0000a5 8308                          st Y, mpr                      ; store saved last character
                                 
                                     ; step 5: wait to create smooth scrolling effect
0000a6 d035                          rcall LCDWrite
0000a7 e119                          ldi waitcnt, WTime
0000a8 dfc1                          rcall wait
0000a9 9508                          ret
                                 
                                 
                                 
                                 
                                 
