00011b dfe6                      	rcall	CLRDM2
00011c 911f                      	pop	r17
00011d 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	Bin2ASCII
                                 ;* Desc:	Converts a binary number into an ASCII 
                                 ;*			text string equivalent. 
                                 ;*			- The binary number needs to be in the mpr
                                 ;*			- The Start Address of where the text will
                                 ;*			 	be placed needs to be in the X Register
                                 ;*			- The count of the characters created are 
                                 ;*				added to the count register
                                 ;*****************************************************************
                                 Bin2ASCII:
00011e 930f                      	push	mpr			; save mpr
00011f 936f                      	push	r			; save r
000120 935f                      	push	q			; save q
000121 93bf                      	push	XH			; save X-pointer
000122 93af                      	push	XL			;
                                 
                                 	; Determine the range of mpr
000123 3604                      	cpi	mpr, 100		; is mpr >= 100
000124 f018                      	brlo	B2A_1			; goto next check
000125 e043                      	ldi	count, 3		; Three chars are written
000126 9613                      	adiw	XL, 3			; Increment X 3 address spaces
000127 c007                      	rjmp	B2A_3			; Continue with program
                                 B2A_1:
000128 300a                      	cpi	mpr, 10		; is mpr >= 10
000129 f018                      	brlo	B2A_2			; Continue with program
00012a e042                      	ldi	count, 2		; Two chars are written
00012b 9612                      	adiw	XL, 2			; Increment X 2 address spaces
00012c c002                      	rjmp	B2A_3			; Continue with program
                                 B2A_2:
00012d 9611                      	adiw	XL, 1			; Increment X 1 address space
00012e e041                      	ldi	count, 1		; One char is written
                                 
                                 B2A_3:
                                 	; Do-While statement that converts Binary to ASCII
00012f d076                      	rcall	div10			; Call the div10 function
000130 e300                      	ldi	mpr, '0'		; Set the base ASCII integer value
000131 0f06                      	add	mpr, r		; Create the ASCII integer value
000132 930e                      	st	-X, mpr		; Load ASCII value to memory
000133 2f05                      	mov	mpr, q		; Set mpr to quotient value
000134 3000                      	cpi	mpr, 0		; does mpr == 0
000135 f7c9                      	brne	B2A_3			; do while (mpr != 0)
                                 
000136 91af                      	pop	XL			; restore X-pointer
000137 91bf                      	pop	XH			;
000138 915f                      	pop	q			; restore q
000139 916f                      	pop	r			; restore r
00013a 910f                      	pop	mpr			; restore mpr
00013b 9508                      	ret				; return from function
                                 
