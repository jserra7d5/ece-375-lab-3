                                 ;*****************************************************************
                                 ;* Func:	LCD_Internal_WriteLn
                                 ;* Desc:	Sends 16 characters of information to the LCD panel
                                 ;*		This is slightly more complicated than it sounds because the
                                 ;*		characters straddle two banks of the LCD memory.
                                 ;*		X is pointing at the first ASCII character to be displayed.
                                 ;*		r17 holds the index of the destination bank in LCD RAM.
                                 ;*		
                                 ;*****************************************************************
                                 LCD_Internal_WriteLn:
00013c 920f                      	push	r0
00013d 921f                      	push	r1
00013e 930f                      	push	mpr
00013f 932f                      	push	r18
000140 933f                      	push	r19
000141 938f                      	push	r24
000142 939f                      	push	r25
000143 93ef                      	push	ZL
000144 93ff                      	push	ZH
                                 
                                 	; r25:24 holds a pointer to the base address of the bitmap array
000145 e093                      	ldi	r25, HIGH(FontBitmap << 1);
000146 e988                      	ldi	r24, LOW(FontBitmap << 1);
                                 
000147 9828                      	cbi	PORTB, 0	; activate slave select
                                 
                                 	; use a counter to track if we are on the top chunk or bottom chunk of each char
000148 e022                      	ldi	r18, 2
                                 
                                 LCD_WL_display_bank:
                                 	; set the column to 0
000149 e100                      	ldi	mpr, lcd_c_disp_set_col_addr_h
00014a d04a                      	rcall	LCD_Internal_WriteCMD
00014b e000                      	ldi	mpr, lcd_c_disp_set_col_addr_l
00014c d048                      	rcall	LCD_Internal_WriteCMD
                                 	; we need to select the appropriate page
00014d eb00                      	ldi	mpr, lcd_c_disp_set_page_addr
00014e 2b01                      	or	mpr, r17	; set the lower 4 bits
00014f d045                      	rcall	LCD_Internal_WriteCMD
                                 
000150 e130                      	ldi	r19, 16	; there are 16 characters per line
                                 
                                 LCD_WL_next_char:
                                 	; get ascii code point & determine location of corresponding character bitmap
000151 900d                      	ld	r0, X+	; this should be a value from 0 to 255
000152 e100                      	ldi	mpr, 16	; there are 16 bytes per character
000153 9e00                      	mul	r0, mpr	; determine the byte offset for the desired char
000154 01fc                      	movw	ZH:ZL, r25:r24	; copy the base address to Z
000155 0de0                      	add	ZL, r0	; finally, compute a pointer to the char bitmap
000156 1df1                      	adc	ZH, r1
                                 
000157 e008                      	ldi	mpr, 8	; there are 8 columns for each character
                                 
                                 	; this is where we display the chunk of character (either a top chunk or bottom chunk)
000158 9004                      	lpm	r0, Z
                                 LCD_WL_next_col:
000159 bc0e                      	out	SPDR, r0
00015a 9a89                      	sbi	PORTF, 1	; set lcd_A0 to specify data mode
00015b 9632                      	adiw	ZH:ZL, 2	; point Z at the next column for this bank
00015c 9004                      	lpm	r0, Z		; this is a sneaky way to save a few clock cycles (since we are waiting for the SPI communication)
                                 LCD_WL_wait_spi:
00015d b41d                      	in	r1, SPSR
00015e fe17                      	sbrs	r1, SPIF	; wait for SPI to finish
00015f cffd                      	rjmp	LCD_WL_wait_spi
                                 
                                 	; keep going until we have displayed all the columns for this character
000160 950a                      	dec	mpr
000161 f7b9                      	brne	LCD_WL_next_col
                                 
                                 	; move to the next character
000162 953a                      	dec	r19
000163 f769                      	brne	LCD_WL_next_char
                                 
                                 	; if we are at the end of the line, configure the bank and starting array offset for the bottom half of each char
000164 952a                      	dec	r18		; did we display both banks yet?
000165 f021                      	breq	LCD_WL_all_done
                                 	; if not, then prepare to display the bottom chunk of each character
000166 951a                      	dec	r17		; decrement the bank number
000167 9750                      	sbiw	XH:XL, 16	; X needs to point back at the beginning character of the line
000168 9601                      	adiw	r25:r24, 1	; increment the base address pointer (now it will point to the second chunk of each char)
000169 cfdf                      	rjmp	LCD_WL_display_bank
                                 
                                 LCD_WL_all_done:
00016a 9a28                      	sbi	PORTB, 0	; deactivate slave select
                                 
00016b 91ff                      	pop	ZH
00016c 91ef                      	pop	ZL
00016d 919f                      	pop	r25
00016e 918f                      	pop	r24
00016f 913f                      	pop	r19
000170 912f                      	pop	r18
000171 910f                      	pop	mpr
000172 901f                      	pop	r1
000173 900f                      	pop	r0
000174 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCD_Internal_ClearLn
                                 ;* Desc:	This function clears two of the LCD memory banks.
                                 ;*		r17 specifies the starting bank to wipe.
                                 ;*****************************************************************
                                 LCD_Internal_ClearLn:
000175 930f                      	push	mpr
000176 921f                      	push	r1
000177 932f                      	push	r18
000178 933f                      	push	r19
                                 
000179 9828                      	cbi	PORTB, 0	; activate slave select
00017a e032                      	ldi	r19, 2	; we are going to clear two banks
                                 
                                 LCD_CLRLN_clear_bank:
                                 	; set the column to 0
00017b e100                      	ldi	mpr, lcd_c_disp_set_col_addr_h
00017c d018                      	rcall	LCD_Internal_WriteCMD
00017d e000                      	ldi	mpr, lcd_c_disp_set_col_addr_l
00017e d016                      	rcall	LCD_Internal_WriteCMD
                                 	; we need to select the appropriate page
00017f eb00                      	ldi	mpr, lcd_c_disp_set_page_addr
000180 2b01                      	or	mpr, r17	; configure the lower 4 bits
000181 d013                      	rcall	LCD_Internal_WriteCMD
                                 
000182 e820                      	ldi	r18, 128	; there are 128 columns on the LCD
000183 2700                      	clr	mpr		; we will fill each column with empty dots (i.e. blank space)
000184 9a89                      	sbi	PORTF, 1	; set lcd_A0 to specify data mode
                                 
                                 LCD_CLRLN_write_col:
000185 bd0e                      	out	SPDR, mpr	; send an empty column to the LCD via SPI
                                 LCD_CLRLN_wait_spi:
000186 b41d                      	in	r1, SPSR
000187 fe17                      	sbrs	r1, SPIF	; wait for SPI to finish
000188 cffd                      	rjmp	LCD_CLRLN_wait_spi
                                 
000189 952a                      	dec	r18
00018a f7d1                      	brne	LCD_CLRLN_write_col	; cycle through all 128 columns
                                 
                                 	; clear the remaining bank if we haven't done so already
00018b 953a                      	dec	r19
00018c f011                      	breq	LCD_CLRLN_all_done
00018d 951a                      	dec	r17		; decrement the bank index
00018e cfec                      	rjmp	LCD_CLRLN_clear_bank
                                 
                                 LCD_CLRLN_all_done:
00018f 9a28                      	sbi	PORTB, 0	; deactivate slave select	
                                 	
                                 	; pop registers...
000190 913f                      	pop	r19
000191 912f                      	pop	r18
000192 901f                      	pop	r1
000193 910f                      	pop	mpr
000194 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCD_Internal_WriteCMD
                                 ;* Desc:	Sends the byte in mpr
                                 ;*		If you use this function directly,
                                 ;*		YOU ARE RESPONSIBLE for handling the slave select line!
                                 ;*****************************************************************
                                 LCD_Internal_WriteCMD:
000195 bd0e                      	out	SPDR, mpr	; initiate transmission
000196 9889                      	cbi	PORTF, 1	; clear lcd_A0 to put the lcd into command mode
                                 				; (this just has to be done before the 8th bit
                                 				; is transmitted via SPI)
                                 LCD_IWC_L1:
000197 b50d                      	in	mpr, SPSR
000198 ff07                      	sbrs	mpr, SPIF	; wait for SPI to finish
000199 cffd                      	rjmp LCD_IWC_L1
00019a 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDDelay
                                 ;* Desc:	A wait loop that will consume roughly 1ms
                                 ;*		Note that a CALL takes 4 cycles on top of this delay.
                                 ;*****************************************************************
                                 LCDDelay:
00019b 930f                      	push	mpr		; 2 cycles
00019c 931f                      	push	r17		; 2 cycles
00019d e01b                      	ldi	r17, 11	; 1 cycle
                                 LCDDelay_L1:
00019e ef02                      	ldi	mpr, 242	; 1 cycle
                                 LCDDelay_L2:
00019f 950a                      	dec	mpr		; 1 cycle
0001a0 f7f1                      	brne	LCDDelay_L2	; 2 cycles on branch, 1 if no branch
0001a1 951a                      	dec	r17		; 1 cycle
0001a2 f7d9                      	brne	LCDDelay_L1	; 2 cycles on branch, 1 if no branch
0001a3 911f                      	pop	r17		; 2 cycles
0001a4 910f                      	pop	mpr		; 2 cycles
0001a5 9508                      	ret			; 4 cycles
                                 
                                 ;*****************************************************************
                                 ;* Func:	div10
                                 ;* Desc:	Divides the value in the mpr by 10 and 
                                 ;*			puts the remainder in the 'r' register
                                 ;*			and the quotient in the 'q' register.
                                 ;*	DO NOT modify this function, trust me, it does
                                 ;*	divide by 10 :)  ~DZ		
                                 ;*****************************************************************
                                 div10:
0001a6 920f                      	push	r0		; Save register
                                 
                                 	; q = mpr / 10 = mpr * 0.000110011001101b
0001a7 2f50                      	mov	q, mpr	; q = mpr * 1.0b
0001a8 9556                      	lsr	q		; q >> 2
0001a9 9556                      	lsr	q		; q = mpr * 0.01b
0001aa 0f50                      	add	q, mpr	; q = (q + mpr) >> 1
0001ab 9556                      	lsr	q		; q = mpr * 0.101b
0001ac 0f50                      	add	q, mpr	; q = (q + mpr) >> 3
0001ad 9556                      	lsr	q
0001ae 9556                      	lsr	q
0001af 9556                      	lsr	q		; q = mpr * 0.001101b
0001b0 0f50                      	add	q, mpr	; q = (q + mpr) >> 1
0001b1 9556                      	lsr	q		; q = mpr * 0.1001101b
0001b2 0f50                      	add	q, mpr	; q = (q + mpr) >> 3
0001b3 9556                      	lsr	q				
0001b4 9556                      	lsr	q
0001b5 9556                      	lsr	q		; q = mpr * 0.0011001101b
0001b6 0f50                      	add	q, mpr	; q = (q + mpr) >> 1
0001b7 9556                      	lsr	q		; q = mpr * 0.10011001101b
0001b8 0f50                      	add	q, mpr	; q = (q + mpr) >> 4
0001b9 9556                      	lsr	q
0001ba 9556                      	lsr	q
0001bb 9556                      	lsr	q
0001bc 9556                      	lsr	q		; q = mpr * 0.000110011001101b
                                 
                                 	; compute the remainder as r = i - 10 * q
                                 	; calculate r = q * 10 = q * 1010b
0001bd 2f65                      	mov	r, q		; r = q * 1
0001be 0f66                      	lsl	r		; r << 2
0001bf 0f66                      	lsl	r		; r = q * 100b
0001c0 0f65                      	add	r, q		; r = (r + q) << 1
0001c1 0f66                      	lsl	r		; r = q * 1010b
0001c2 2e06                      	mov	r0, r		; r0 = 10 * q
0001c3 2f60                      	mov	r, mpr	; r = mpr
0001c4 1960                      	sub	r, r0		; r = mpr - 10 * q
                                 
                                 	; Fix any errors that occur
                                 div10_1:
0001c5 306a                      	cpi	r, 10		; Compare with 10
0001c6 f018                      	brlo	div10_2	; do nothing if r < 10
0001c7 9553                      	inc	q		; fix quotient
0001c8 506a                      	subi	r, 10		; fix remainder
0001c9 cffb                      	rjmp	div10_1	; Continue until error is corrected
                                 
                                 div10_2:
0001ca 900f                      	pop	r0		; Restore registers
0001cb 9508                      	ret			; Return from function
                                 
