                                 ;*	Program Initialization
                                 ;***********************************************************
                                 INIT:							; The initialization routine
                                 		; initialize our stack pointer
000056 ef0f                      		ldi mpr, low(RAMEND)
000057 bf0d                      		out SPL, mpr
000058 e00a                      		ldi mpr, high(RAMEND)
000059 bf0e                      		out SPH, mpr
                                 
                                 		; initialize Port D for input
00005a e000                      		ldi mpr, $00
00005b b90a                      		out DDRD, mpr
00005c ef0f                      		ldi mpr, $FF
00005d b90b                      		out PORTD, mpr
                                 
                                 		; Initialize LCD Display
00005e d057                      		rcall LCDInit
00005f d02d                      		rcall CLEAR
                                 
                                 
                                 		; NOTE that there is no RET or RJMP from INIT,
                                 		; this is because the next instruction executed is the
                                 		; first instruction of the main program
                                 
                                 ;***********************************************************
                                 ;*	Main Program
                                 ;***********************************************************
                                 MAIN:							; The Main program
                                 		; first lets grab our inputs
000060 b109                      		in mpr, PIND
000061 7b00                      		andi mpr, (1<<clear_button | 1<<display_button | 1<<scroll_button)
                                 
                                 		; check if our clear button is pressed, if so, clear the screen
000062 ff04                      		sbrs mpr, clear_button
000063 d029                      		rcall CLEAR
                                 
                                 		; check if our display button is pressed, if so, display the lines
000064 ff05                      		sbrs mpr, display_button
000065 d013                      		rcall DISPLAY
                                 
                                 		; check if our scroll button is pressed, if so, call the scroll subroutine
000066 ff07                      		sbrs mpr, scroll_button
000067 d031                      		rcall SCROLL
000068 cff7                      		rjmp	MAIN			; jump back to main and create an infinite
                                 								; while loop.  Generally, every main program is an
                                 								; infinite while loop, never let the main program
                                 								; just run off
                                 
