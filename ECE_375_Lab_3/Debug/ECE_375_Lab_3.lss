                                 ;*	The functions below here are intended for internal use only.
                                 ;*****************************************************************
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCD_Internal_WriteLn
                                 ;* Desc:	Sends 16 characters of information to the LCD panel
                                 ;*		This is slightly more complicated than it sounds because the
                                 ;*		characters straddle two banks of the LCD memory.
                                 ;*		X is pointing at the first ASCII character to be displayed.
                                 ;*		r17 holds the index of the destination bank in LCD RAM.
                                 ;*		
                                 ;*****************************************************************
                                 LCD_Internal_WriteLn:
000138 920f                      	push	r0
000139 921f                      	push	r1
00013a 930f                      	push	mpr
00013b 932f                      	push	r18
00013c 933f                      	push	r19
00013d 938f                      	push	r24
00013e 939f                      	push	r25
00013f 93ef                      	push	ZL
000140 93ff                      	push	ZH
                                 
                                 	; r25:24 holds a pointer to the base address of the bitmap array
000141 e093                      	ldi	r25, HIGH(FontBitmap << 1);
000142 e980                      	ldi	r24, LOW(FontBitmap << 1);
                                 
000143 9828                      	cbi	PORTB, 0	; activate slave select
                                 
                                 	; use a counter to track if we are on the top chunk or bottom chunk of each char
000144 e022                      	ldi	r18, 2
                                 
                                 LCD_WL_display_bank:
                                 	; set the column to 0
000145 e100                      	ldi	mpr, lcd_c_disp_set_col_addr_h
000146 d04a                      	rcall	LCD_Internal_WriteCMD
000147 e000                      	ldi	mpr, lcd_c_disp_set_col_addr_l
000148 d048                      	rcall	LCD_Internal_WriteCMD
                                 	; we need to select the appropriate page
000149 eb00                      	ldi	mpr, lcd_c_disp_set_page_addr
00014a 2b01                      	or	mpr, r17	; set the lower 4 bits
00014b d045                      	rcall	LCD_Internal_WriteCMD
                                 
00014c e130                      	ldi	r19, 16	; there are 16 characters per line
                                 
                                 LCD_WL_next_char:
                                 	; get ascii code point & determine location of corresponding character bitmap
00014d 900d                      	ld	r0, X+	; this should be a value from 0 to 255
00014e e100                      	ldi	mpr, 16	; there are 16 bytes per character
00014f 9e00                      	mul	r0, mpr	; determine the byte offset for the desired char
000150 01fc                      	movw	ZH:ZL, r25:r24	; copy the base address to Z
000151 0de0                      	add	ZL, r0	; finally, compute a pointer to the char bitmap
000152 1df1                      	adc	ZH, r1
                                 
000153 e008                      	ldi	mpr, 8	; there are 8 columns for each character
                                 
                                 	; this is where we display the chunk of character (either a top chunk or bottom chunk)
000154 9004                      	lpm	r0, Z
                                 LCD_WL_next_col:
000155 bc0e                      	out	SPDR, r0
000156 9a89                      	sbi	PORTF, 1	; set lcd_A0 to specify data mode
000157 9632                      	adiw	ZH:ZL, 2	; point Z at the next column for this bank
000158 9004                      	lpm	r0, Z		; this is a sneaky way to save a few clock cycles (since we are waiting for the SPI communication)
                                 LCD_WL_wait_spi:
000159 b41d                      	in	r1, SPSR
00015a fe17                      	sbrs	r1, SPIF	; wait for SPI to finish
00015b cffd                      	rjmp	LCD_WL_wait_spi
                                 
                                 	; keep going until we have displayed all the columns for this character
00015c 950a                      	dec	mpr
00015d f7b9                      	brne	LCD_WL_next_col
                                 
                                 	; move to the next character
00015e 953a                      	dec	r19
00015f f769                      	brne	LCD_WL_next_char
                                 
                                 	; if we are at the end of the line, configure the bank and starting array offset for the bottom half of each char
000160 952a                      	dec	r18		; did we display both banks yet?
000161 f021                      	breq	LCD_WL_all_done
                                 	; if not, then prepare to display the bottom chunk of each character
000162 951a                      	dec	r17		; decrement the bank number
000163 9750                      	sbiw	XH:XL, 16	; X needs to point back at the beginning character of the line
000164 9601                      	adiw	r25:r24, 1	; increment the base address pointer (now it will point to the second chunk of each char)
000165 cfdf                      	rjmp	LCD_WL_display_bank
                                 
                                 LCD_WL_all_done:
000166 9a28                      	sbi	PORTB, 0	; deactivate slave select
                                 
000167 91ff                      	pop	ZH
000168 91ef                      	pop	ZL
000169 919f                      	pop	r25
00016a 918f                      	pop	r24
00016b 913f                      	pop	r19
00016c 912f                      	pop	r18
00016d 910f                      	pop	mpr
00016e 901f                      	pop	r1
00016f 900f                      	pop	r0
000170 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCD_Internal_ClearLn
                                 ;* Desc:	This function clears two of the LCD memory banks.
                                 ;*		r17 specifies the starting bank to wipe.
                                 ;*****************************************************************
                                 LCD_Internal_ClearLn:
000171 930f                      	push	mpr
000172 921f                      	push	r1
000173 932f                      	push	r18
000174 933f                      	push	r19
                                 
000175 9828                      	cbi	PORTB, 0	; activate slave select
000176 e032                      	ldi	r19, 2	; we are going to clear two banks
                                 
                                 LCD_CLRLN_clear_bank:
                                 	; set the column to 0
000177 e100                      	ldi	mpr, lcd_c_disp_set_col_addr_h
000178 d018                      	rcall	LCD_Internal_WriteCMD
000179 e000                      	ldi	mpr, lcd_c_disp_set_col_addr_l
00017a d016                      	rcall	LCD_Internal_WriteCMD
                                 	; we need to select the appropriate page
00017b eb00                      	ldi	mpr, lcd_c_disp_set_page_addr
00017c 2b01                      	or	mpr, r17	; configure the lower 4 bits
00017d d013                      	rcall	LCD_Internal_WriteCMD
                                 
00017e e820                      	ldi	r18, 128	; there are 128 columns on the LCD
00017f 2700                      	clr	mpr		; we will fill each column with empty dots (i.e. blank space)
000180 9a89                      	sbi	PORTF, 1	; set lcd_A0 to specify data mode
                                 
                                 LCD_CLRLN_write_col:
000181 bd0e                      	out	SPDR, mpr	; send an empty column to the LCD via SPI
                                 LCD_CLRLN_wait_spi:
000182 b41d                      	in	r1, SPSR
000183 fe17                      	sbrs	r1, SPIF	; wait for SPI to finish
000184 cffd                      	rjmp	LCD_CLRLN_wait_spi
                                 
000185 952a                      	dec	r18
000186 f7d1                      	brne	LCD_CLRLN_write_col	; cycle through all 128 columns
                                 
                                 	; clear the remaining bank if we haven't done so already
000187 953a                      	dec	r19
000188 f011                      	breq	LCD_CLRLN_all_done
000189 951a                      	dec	r17		; decrement the bank index
00018a cfec                      	rjmp	LCD_CLRLN_clear_bank
                                 
                                 LCD_CLRLN_all_done:
00018b 9a28                      	sbi	PORTB, 0	; deactivate slave select	
                                 	
                                 	; pop registers...
00018c 913f                      	pop	r19
00018d 912f                      	pop	r18
00018e 901f                      	pop	r1
00018f 910f                      	pop	mpr
000190 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCD_Internal_WriteCMD
                                 ;* Desc:	Sends the byte in mpr
                                 ;*		If you use this function directly,
                                 ;*		YOU ARE RESPONSIBLE for handling the slave select line!
                                 ;*****************************************************************
                                 LCD_Internal_WriteCMD:
000191 bd0e                      	out	SPDR, mpr	; initiate transmission
000192 9889                      	cbi	PORTF, 1	; clear lcd_A0 to put the lcd into command mode
                                 				; (this just has to be done before the 8th bit
                                 				; is transmitted via SPI)
                                 LCD_IWC_L1:
000193 b50d                      	in	mpr, SPSR
000194 ff07                      	sbrs	mpr, SPIF	; wait for SPI to finish
000195 cffd                      	rjmp LCD_IWC_L1
000196 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDDelay
                                 ;* Desc:	A wait loop that will consume roughly 1ms
                                 ;*		Note that a CALL takes 4 cycles on top of this delay.
                                 ;*****************************************************************
                                 LCDDelay:
000197 930f                      	push	mpr		; 2 cycles
000198 931f                      	push	r17		; 2 cycles
000199 e01b                      	ldi	r17, 11	; 1 cycle
                                 LCDDelay_L1:
00019a ef02                      	ldi	mpr, 242	; 1 cycle
                                 LCDDelay_L2:
00019b 950a                      	dec	mpr		; 1 cycle
00019c f7f1                      	brne	LCDDelay_L2	; 2 cycles on branch, 1 if no branch
00019d 951a                      	dec	r17		; 1 cycle
00019e f7d9                      	brne	LCDDelay_L1	; 2 cycles on branch, 1 if no branch
00019f 911f                      	pop	r17		; 2 cycles
0001a0 910f                      	pop	mpr		; 2 cycles
0001a1 9508                      	ret			; 4 cycles
                                 
                                 ;*****************************************************************
                                 ;* Func:	div10
                                 ;* Desc:	Divides the value in the mpr by 10 and 
                                 ;*			puts the remainder in the 'r' register
                                 ;*			and the quotient in the 'q' register.
                                 ;*	DO NOT modify this function, trust me, it does
                                 ;*	divide by 10 :)  ~DZ		
                                 ;*****************************************************************
                                 div10:
0001a2 920f                      	push	r0		; Save register
                                 
                                 	; q = mpr / 10 = mpr * 0.000110011001101b
0001a3 2f50                      	mov	q, mpr	; q = mpr * 1.0b
0001a4 9556                      	lsr	q		; q >> 2
0001a5 9556                      	lsr	q		; q = mpr * 0.01b
0001a6 0f50                      	add	q, mpr	; q = (q + mpr) >> 1
0001a7 9556                      	lsr	q		; q = mpr * 0.101b
0001a8 0f50                      	add	q, mpr	; q = (q + mpr) >> 3
0001a9 9556                      	lsr	q
0001aa 9556                      	lsr	q
0001ab 9556                      	lsr	q		; q = mpr * 0.001101b
0001ac 0f50                      	add	q, mpr	; q = (q + mpr) >> 1
0001ad 9556                      	lsr	q		; q = mpr * 0.1001101b
0001ae 0f50                      	add	q, mpr	; q = (q + mpr) >> 3
0001af 9556                      	lsr	q				
0001b0 9556                      	lsr	q
0001b1 9556                      	lsr	q		; q = mpr * 0.0011001101b
0001b2 0f50                      	add	q, mpr	; q = (q + mpr) >> 1
0001b3 9556                      	lsr	q		; q = mpr * 0.10011001101b
0001b4 0f50                      	add	q, mpr	; q = (q + mpr) >> 4
0001b5 9556                      	lsr	q
0001b6 9556                      	lsr	q
0001b7 9556                      	lsr	q
0001b8 9556                      	lsr	q		; q = mpr * 0.000110011001101b
                                 
                                 	; compute the remainder as r = i - 10 * q
                                 	; calculate r = q * 10 = q * 1010b
0001b9 2f65                      	mov	r, q		; r = q * 1
0001ba 0f66                      	lsl	r		; r << 2
0001bb 0f66                      	lsl	r		; r = q * 100b
0001bc 0f65                      	add	r, q		; r = (r + q) << 1
0001bd 0f66                      	lsl	r		; r = q * 1010b
0001be 2e06                      	mov	r0, r		; r0 = 10 * q
0001bf 2f60                      	mov	r, mpr	; r = mpr
0001c0 1960                      	sub	r, r0		; r = mpr - 10 * q
                                 
                                 	; Fix any errors that occur
                                 div10_1:
0001c1 306a                      	cpi	r, 10		; Compare with 10
0001c2 f018                      	brlo	div10_2	; do nothing if r < 10
0001c3 9553                      	inc	q		; fix quotient
0001c4 506a                      	subi	r, 10		; fix remainder
0001c5 cffb                      	rjmp	div10_1	; Continue until error is corrected
                                 
                                 div10_2:
0001c6 900f                      	pop	r0		; Restore registers
0001c7 9508                      	ret			; Return from function
                                 
