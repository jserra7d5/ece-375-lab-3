                                 ;*****************************************************************
                                 ;* Func:	Bin2ASCII
                                 ;* Desc:	Converts a binary number into an ASCII 
                                 ;*			text string equivalent. 
                                 ;*			- The binary number needs to be in the mpr
                                 ;*			- The Start Address of where the text will
                                 ;*			 	be placed needs to be in the X Register
                                 ;*			- The count of the characters created are 
                                 ;*				added to the count register
                                 ;*****************************************************************
                                 Bin2ASCII:
00011a 930f                      	push	mpr			; save mpr
00011b 936f                      	push	r			; save r
00011c 935f                      	push	q			; save q
00011d 93bf                      	push	XH			; save X-pointer
00011e 93af                      	push	XL			;
                                 
                                 	; Determine the range of mpr
00011f 3604                      	cpi	mpr, 100		; is mpr >= 100
000120 f018                      	brlo	B2A_1			; goto next check
000121 e043                      	ldi	count, 3		; Three chars are written
000122 9613                      	adiw	XL, 3			; Increment X 3 address spaces
000123 c007                      	rjmp	B2A_3			; Continue with program
                                 B2A_1:
000124 300a                      	cpi	mpr, 10		; is mpr >= 10
000125 f018                      	brlo	B2A_2			; Continue with program
000126 e042                      	ldi	count, 2		; Two chars are written
000127 9612                      	adiw	XL, 2			; Increment X 2 address spaces
000128 c002                      	rjmp	B2A_3			; Continue with program
                                 B2A_2:
000129 9611                      	adiw	XL, 1			; Increment X 1 address space
00012a e041                      	ldi	count, 1		; One char is written
                                 
                                 B2A_3:
                                 	; Do-While statement that converts Binary to ASCII
00012b d076                      	rcall	div10			; Call the div10 function
00012c e300                      	ldi	mpr, '0'		; Set the base ASCII integer value
00012d 0f06                      	add	mpr, r		; Create the ASCII integer value
00012e 930e                      	st	-X, mpr		; Load ASCII value to memory
00012f 2f05                      	mov	mpr, q		; Set mpr to quotient value
000130 3000                      	cpi	mpr, 0		; does mpr == 0
000131 f7c9                      	brne	B2A_3			; do while (mpr != 0)
                                 
000132 91af                      	pop	XL			; restore X-pointer
000133 91bf                      	pop	XH			;
000134 915f                      	pop	q			; restore q
000135 916f                      	pop	r			; restore r
000136 910f                      	pop	mpr			; restore mpr
000137 9508                      	ret				; return from function
                                 
